{
   "background": {
      "persistent": false,
      "scripts": [ "install.js" ]
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "wckf.js", "jquery.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_idle"
   } ],
   "default_locale": "en",
   "description": "__MSG_extDesc__",
   "icons": {
      "128": "icon-128.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": "__MSG_extName__",
   "options_page": "settings.html",
   "page_action": {
      "default_icon": {
         "19": "icon-19.png",
         "38": "icon-38.png"
      }
   },
   "permissions": [ "storage", "notifications", "tabs" ],
   "version": "1.0.1"
}
